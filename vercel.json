{
  "builds": [{
    "src": "./server.js",
    "use": "@vercel/node"
  }],
  "routes": [{
    "src": "/create_preference",
    "dest": "https://electro-ecommerce-back.vercel.app/create_preference",
    "methods": ["POST", "OPTIONS"],
    "headers": {
      "Access-Control-Allow-Origin": "https://ecommerce-electro.vercel.app",
      "Access-Control-Allow-Methods": "POST, OPTIONS",
      "Access-Control-Allow-Headers": "Content-Type"
    }
  }]
}